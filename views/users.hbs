{{#if alertMessage}}
<div class="alert alert-danger" role="alert">
  {{alertMessage}}
</div>
{{/if}}

<h2>{{this.message}}</h2>
<div class="list-section">
    <table>
        <tr>
            <th>Username</th>
            <th>Role</th>
        </tr>
        {{#each users}}
        <tr>
            <td>{{username}}</td>
            <td>{{rolename}}</td>
        </tr>
        {{/each}}
    </table>
</div>